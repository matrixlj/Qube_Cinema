<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <targets>
    <target name="eventlog" xsi:type="EventLog" log="Qube" source="Qube" layout="${message}${newline}${exception:format=ToString}"/>
	<target name="network" xsi:type="Network" address="udp://127.0.0.1:5000" layout="${log4jxmlevent:includeCallSite=true:includeNLogData=false}"/>
  </targets>

  <rules>
    <logger name="*" minLevel="Info" writeTo="eventlog,network" />
  </rules>
</nlog>